def calc(n1,n2,op):
if op=='+':
 r=n1+n2
elif op=='-':
 r=n1-n2
elif op=='*':
 r=n1*n2
elif op=='/':
 r=n1/n2
elif op=='^':
 r=n1**n2
else:
 print("Invalid operator")
return r
n1=float(input("Enter 1st number:"))
n2=float(input("Enter 2nd number:"))
op=input("Enter the operator(+,-,*,/,^)")
r=calc(n1,n2,op)
print(f"{n1} {op} {n2}={r}")
